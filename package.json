{
  "name": "redoc-express-maintained",
  "version": "1.0.0",
  "description": "Community-maintained fork of redoc-express. Express middleware for OpenAPI/Swagger documentation with compatibility fixes.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.js",
      "types": "./dist/plugins/index.d.ts"
    }
  },
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "test": "jest",
    "build": "tsc"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moltivie/redoc-express-maintained.git"
  },
  "keywords": [
    "redoc-swagger-ui-express-middleware"
  ],
  "author": "Moltivie",
  "contributors": [
    "Aung Myo Kyaw (original author)",
    "Muhammed Tanrikulu (contributor)",
    "Aaron Hipple (contributor)",
    "Moltivie (maintainer)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Moltivie/redoc-express-maintained/issues"
  },
  "homepage": "https://github.com/Moltivie/redoc-express-maintained#readme",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}